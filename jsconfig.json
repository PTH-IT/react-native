{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "jsx": "react",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": ".",
        "paths": {
            "~/*": ["src/*"],
          "reduxreducers/*": ["storeredux/reducers/*"],
          "action/*": ["storeredux/action/*"],
          "constants/*": ["storeredux/constants/*"],
          "config/*": ["config/*"],
          "components/*": ["src/components/*"],
          "API/*": ["src/api/*"],
        }
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}